5.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = log(pow(y,-sqrt(fabs(x))));
 
 b = x-y/2;
 
 c = pow(sin(atan(z)),2);
 
 s = a*b+c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

