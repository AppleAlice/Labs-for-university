11.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = pow(y,pow(fabs(x),1/3));
 
 b = pow(cos(y),3)*fabs(x-y)*(1+pow(sin(z),2)/sqrt(x+y);
 
 c = exp(fabs(x-y))+x/2;
 
 s = a+b/c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

------------------------------------------------------------------------------------------------------

12.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = pow(2,pow(y,x))+pow(pow(3,x),y);
 
 b = y*(atan(z)-1/3);
 
 c = fabs(x)+1/(pow(y,2)+1);
 
 s = a-b/c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

-------------------------------------------------------------------------------------------------------------

13.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = pow(y+pow(x-1,1/3),1/4);
 
 b = fabs(x-y);
 
 c = pow(sin(z),2)+tan(z);
 
 s = a/(b*c);
 
 cout << "Result s= " << s << endl;
 
return 0;

}

----------------------------------------------------------------------------------------------------

14.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = pow(y,x+1)/(pow(fabs(y-2),1/3)+3);
 
 b = (x+y/2)/(2*fabs(x+y));
 
 c = pow(x+1,(-1)/sin(z));
 
 s = a+b*c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

-------------------------------------------------------------------------------------------------------

15.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = (pow(x,y+1)+exp(y-1))*(1+fabs(y-x));
 
 b = 1+x*fabs(y-tan(z));
 
 c = pow(fabs(y-x),2)/2-pow(fabs(y-x),3)/3;
 
 s = a/b+c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}
