7.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = 5*atan(x);
 
 b = 1/4*acos(x)*(x+3*fabs(x-y)+pow(x,z));
 
 c = fabc(x-y)*z+pow(x,2);
 
 s = a-b/c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

--------------------------------------------------------------------------

8.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = exp(fabs(x-y))*pow(fabs(x-y),x+y);
 
 b = atan(x)+atan(z);
 
 c = pow(pow(x,6)+pow(log(y),2),1/3);
 
 s = a/b+c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

-----------------------------------------------------------------------------------------------

9.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = fabs(pow(x,y/x)-pow(y/x,1/3));
 
 b = (y-x)*(cos(y)-z/(y-x));
 
 c = 1+pow(y-x,2);
 
 s = a+b/c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

-----------------------------------------------------------------------------------------------

10.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = pow(2,-x);
 
 b = sqrt(x+pow(fabs(y),1/4));
 
 c = pow(exp(x-1/sin(z),1/3);
 
 s = a*b*c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

