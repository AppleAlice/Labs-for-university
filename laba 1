1.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = 2*cos(x-2/3);
 
 b = 1/2+pow(sin(y),2);
 
 c = 1+pow(z,2)/(3-pow(z,2)/5);
 
 s = a/b*c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

---------------------------------------------------------------------------------------

2.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = pow(9+pow(x-y,2));
 
 b = pow(x,2)+pow(y,2)+2;
 
 c = exp(fabs(x-y))*pow(tan(z),3);
 
 s = a/b+c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

------------------------------------------------------------------

3.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = 1+pow(sin(x+y),2)*pow(x,fabs(y));
 
 b = fabs(x-2*y/(1+pow(x,2)*pow(y,2));
 
 c = pow(cos(atan(1/z)),2);
 
 s = a/b+c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

------------------------------------------------------------------------------------------------

4.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = 1+2*pow(sin(y),2);
 
 b = fabs(cos(x)-cos(y));
 
 c = 1+z+pow(z,2)/2+pow(z,3)/3+pow(z,4)/4;
 
 s = pow(b,a)*c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

