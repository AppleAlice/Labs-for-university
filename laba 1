6.

#include <iostream.h>

#include <math.h>

int main ()

{

 double x,y,z,a,b,c,s;
 
cout << "Vvedite x: ";

 cin >> x;
 
cout << "Vvedite y: ";

 cin >> y;
 
cout << "Vvedite z: ";

 cin >> z;
 
 a = pow(x,1/3)+pow(x,y+2);
 
 b = sqrt(10*a);
 
 c = pow(asin(z),2)-fabs(x-y);
 
 s = b*c;
 
 cout << "Result s= " << s << endl;
 
return 0;

}

